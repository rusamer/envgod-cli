  },
  "devDependencies": {
    "@types/keytar": "^4.0.2",
    "@types/node": "^20.11.24",
    "eslint": "^8.57.0",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3",
    "vitest": "^1.3.1"
  },
  "engines": {
    "node": ">=20.17.0"
  }
